{
    "name": "tototoshi/staticmock",
    "description": "A mockery-like DSL to replace static method in test.",
    "license": "BSD-3-Clause",
    "suggest": {
        "ext-runkit": "https://github.com/runkit7/runkit7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.3",
        "herrera-io/runkit-hint": "*",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-0": { "StaticMock": ["src/"] }
    },
    "autoload-dev": {
        "psr-0": { "StaticMock": ["test/"] }
    }
}
